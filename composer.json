{
    "name": "digikraaft/laravel-dashboard-stripe-customers-tile",
    "description": "A tile to show list of Stripe Customers on Laravel Dashboard",
    "keywords": [
        "digikraaft",
        "laravel-dashboard",
        "stripe-dashboard",
        "stripe"
    ],
    "homepage": "https://github.com/digikraaft/laravel-dashboard-stripe-customers-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Oladoyinbo",
            "email": "dev@digitalkraaft.com",
            "homepage": "https://digitalkraaft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/console": "^8.0",
        "illuminate/pagination": "^8.0",
        "illuminate/support": "^8.0",
        "spatie/laravel-dashboard": "^2.0",
        "stripe/stripe-php": "^7.46"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Digikraaft\\StripeCustomersTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digikraaft\\StripeCustomersTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digikraaft\\StripeCustomersTile\\StripeCustomersTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
